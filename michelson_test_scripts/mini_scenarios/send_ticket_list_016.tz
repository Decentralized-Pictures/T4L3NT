# This contract mints tickets into a list and sends them to a contract specified in the parameter.
parameter address ;
storage unit;
code { CAR ;
       CONTRACT (list (ticket string)) ;
       ASSERT_SOME ;
       PUSH mutez 0 ;
       NIL (ticket string) ;
       PUSH nat 1 ;
       PUSH string "Ticket" ;
       TICKET ;
       ASSERT_SOME ;
       CONS ;
       TRANSFER_TOKENS ;
       NIL operation ;
       SWAP ;
       CONS ;
       UNIT ;
       SWAP ;
       PAIR }